   <soggetto tipo="PROXY" nome="Comune">

        <connettore tipo="http" nome="PdDComune">
             <property nome="location" valore="http://127.0.0.1:8080/openspcoop2/proxy/PA" />
        </connettore>

   </soggetto>

</registro-servizi>
