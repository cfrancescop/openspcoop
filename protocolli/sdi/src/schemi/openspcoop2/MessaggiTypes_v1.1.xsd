			<xsd:enumeration value="DT">
				<xsd:annotation>
					<xsd:documentation>
						DT = Notifica decorrenza termini 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AT">
				<xsd:annotation>
					<xsd:documentation>
						AT = Attestazione di avvenuta trasmissione della fattura con impossibilit√† di recapito 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
