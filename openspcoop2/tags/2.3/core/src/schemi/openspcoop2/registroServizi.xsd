	<!-- operation -->
	<xsd:complexType name="operation">
		<xsd:sequence>
			<xsd:element maxOccurs="1" minOccurs="0" name="message-input" type="message"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="message-output" type="message"/>
		</xsd:sequence>
		<!-- start-extensions -->
		<xsd:attribute name="prof-azione" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:appinfo>
					<linkit:generator
						xmlns:linkit="http://www.link.it/project_generator">
						<linkit:ws-field-mapping filter="false"/>
					</linkit:generator>
				</xsd:appinfo>
